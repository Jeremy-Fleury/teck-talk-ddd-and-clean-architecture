{
	"openapi": "3.0.0",
	"paths": {
		"/": {
			"get": {
				"operationId": "AppController_health",
				"parameters": [],
				"responses": {
					"200": {
						"description": "Liveness response: `status=\"ok\"`.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/HealthOutputDto"
								}
							}
						}
					}
				},
				"summary": "Health check",
				"tags": ["App"]
			}
		},
		"/user": {
			"get": {
				"operationId": "UserController_user",
				"parameters": [],
				"responses": {
					"200": {
						"description": "Current user found and returned.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserOutputDto"
								}
							}
						}
					}
				},
				"security": [
					{
						"bearer": []
					}
				],
				"tags": ["User"]
			}
		}
	},
	"info": {
		"title": "@org/api",
		"description": "Boilerplate for NestJS projects",
		"version": "1.0.0",
		"contact": {}
	},
	"tags": [],
	"servers": [],
	"components": {
		"securitySchemes": {
			"bearer": {
				"scheme": "bearer",
				"bearerFormat": "JWT",
				"type": "http"
			}
		},
		"schemas": {
			"HealthOutputDto": {
				"type": "object",
				"properties": {
					"status": {
						"type": "string",
						"description": "The status of the health check",
						"example": "ok"
					},
					"secret": {
						"type": "string",
						"description": "The secret of the health check",
						"example": "tata"
					}
				},
				"required": ["status", "secret"]
			},
			"UserOutputDto": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string",
						"description": "The user id",
						"example": "019ad951-368a-7de5-b7ba-add19cfd187b"
					},
					"email": {
						"type": "string",
						"description": "The user email",
						"example": "hello@acme.com",
						"nullable": true
					},
					"firstName": {
						"type": "string",
						"description": "First name for the user",
						"example": "John",
						"nullable": true
					},
					"lastName": {
						"type": "string",
						"description": "Last name for the user",
						"example": "Doe",
						"nullable": true
					}
				},
				"required": ["id", "email", "firstName", "lastName"]
			}
		}
	}
}
